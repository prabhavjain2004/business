{
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/python",
      "config": {
        "distDir": "static"
      }
    },
    {
      "src": "myproject/myproject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "myproject/myproject/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}